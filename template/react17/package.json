{
  "browserslist":    {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production":  [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies":    {
    "react":      "^17.0.1",
    "react-dom":  "^17.0.1",
    "typescript": "^4.1.5"
  },
  "description":     "This is a project that allows code to be (safely) fetched on demand from a server. The url holding the code is including in the json",
  "devDependencies": {
    "eslint":                        "^8.52.0",
    "typescript":                         "^5.2.2",
    "@types/jest":                        "29.1.1",
    "@types/node":                        "^18.0.0",
    "jest":                               "29.1.1",
    "ts-jest":                            "^29.1.1",
    "@testing-library/jest-dom":          "^6.1.4",
    "@types/react":                       "^17.0.0",
    "@types/react-dom":                   "^17.0.0",
    "@testing-library/react":             "^11.2.2",
    "@testing-library/user-event":        "^12.2.2",
    "enzyme":                             "^3.11.0",
    "@types/enzyme":                      "^3.10.8",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.2"
  },
  "files":           [
    "LICENSE.md",
    "README.md",
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/"
  ],
  "keywords":        [
    "functionalprogramming",
    "lens",
    "react",
    "optics"
  ],
  "license":         "MIT",
  "main":            "dist/index",
  "types":           "dist/index",
  "repository":      "github:focuson-ts/focuson",
  "scripts":         {
    "start":      "react-scripts start",
    "test":       "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook":  "start-storybook -p 6006 -s public"
  },
  "version":         "${version}",
  "eslintConfig":    {
    "extends":   [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
