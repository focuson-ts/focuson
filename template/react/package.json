{
  "browserslist":    {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production":  [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies":    {
    "react":      "16.14.0",
    "react-dom":  "16.14.0",
    "typescript": "^4.1.5"
  },
  "description":     "This is a project that allows code to be (safely) fetched on demand from a server. The url holding the code is including in the json",
  "devDependencies": {
    "@types/react":                   "16.14.0",
    "@testing-library/jest-dom":      "^5.11.6",
    "@testing-library/react":         "^11.2.2",
    "@testing-library/user-event":    "^12.2.2",
    "@types/jest":                    "^26.0.15",
    "@types/node":                    "^14.14.9",
    "enzyme-adapter-react-16":        "^1.15.6",
    "enzyme": "^3.11.0",
    "@types/enzyme":                  "^3.10.12",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "jest":                           "26.6.0",
    "jest-dom":                       "2.1.1",
    "react-scripts":                  "^4.0.1",
    "ts-jest":                        "^26.4.4"
  },
  "files":           [
    "LICENSE.md",
    "README.md",
    "dist/index.js",
    "dist/index.d.ts",
    "dist/src/"
  ],
  "keywords":        [
    "functionalprogramming",
    "lens",
    "react",
    "optics"
  ],
  "license":         "MIT",
  "main":            "dist/index",
  "types":           "dist/index",
  "repository":      "github:focuson-ts/focuson",
  "scripts":         {
    "start":      "react-scripts start",
    "test":       "jest --config jest.config.json",
    "test:watch": "npm run test -- --watch",
    "storybook":  "start-storybook -p 6006 -s public"
  },
  "version":     "${version}",
  "eslintConfig":    {
    "extends":   [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
